<table>
  <tr>
    <th>
      ID
    </th>
    <th>
      Last Name
    </th>
    <th>
      First Name
    </th>
    <th>
      Email
    </th>
    <th>
      UID
    </th>
  </tr>
  <%- for user in @users %>
    <tr>
      <td>
        <%= user.id %>
      </td>
      <td>
        <%= user.surname %>
      </td>
      <td>
        <%= user.forename %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.uid %>
      </td>
    </tr>
  <%- end %>
</table>
